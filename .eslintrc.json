{
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": "react-native",
    "rules": {
      "react-native/no-color-literals": 0,
      "no-invalid-this": 0,
      "react-native/no-inline-styles": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": [0],
      "react/no-deprecated": 0,
      "react/prefer-stateless-function": 0
    }
}
